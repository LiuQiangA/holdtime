.message-head{
    width: 94%;
    height: 80px;
    line-height: 80px;
    padding: 0 3%;
    float: left;
    color: #fff;
    font-size: 20px;
    /* box-shadow: 1px 1px 5px 1px #102337; */
    background: url(../../static/images/header-bottom-bg.jpg) repeat-x bottom;
  }
  .getMD{
    width: 80px;
    height: 50px;
    float: right;
    background: url(../../static/images/MD-bg.png) no-repeat;
    background-size: 100% 100%;
    margin-top: 14px;
  }
  .getMonth{
    width: 42%;
    height: 50px;
    line-height: 48px;
    color: #2c8063;
    font-size: 20px;
    display: block;
    float: left;
    text-align: center;
    margin-left: 8%;
  }
  .getDay{
    width: 46%;
    height: 50px;
    line-height: 48px;
    color: #2c8063;
    font-size: 20px;
    display: block;
    float: left;
    text-align: center;
  }
  .fabuNew {
    float: right;
    color: #fff;
  }
  
  .message-cont-head-t {
    float: left;
    height: 39px;
    border-bottom: 4px solid #00b77a;
    color: #00b77a;
  }
  
  .message-cont-head {
    width: 100%;
    height: 40px;
    line-height: 40px;
    margin: 20px 0;
    float: left;
    border-bottom: 1px solid #00b77a;
    font-size: 18px;
  }
  
  .message-cont {
    width: 95%;
    padding: 0 3%;
    overflow-y: auto;
  }
  
  .message-body {
    width: 100%;
    float: left;
  }
  
  .message {
    width: 100%;
    float: left;
    overflow: hidden;
  }